{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "fc-rckb-frontend": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "fc",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/fc-rckb-frontend",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss",
              "src/assets/fonts/feather/feather.css",
              "node_modules/flatpickr/dist/flatpickr.min.css",
              "node_modules/quill/dist/quill.core.css",
              "node_modules/highlightjs/styles/vs2015.css",
              "src/assets/scss/theme.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "node_modules/@shopify/draggable/lib/es5/draggable.bundle.legacy.js",
              "node_modules/autosize/dist/autosize.min.js",
              "node_modules/chart.js/dist/Chart.min.js",
              "node_modules/dropzone/dist/min/dropzone.min.js",
              "node_modules/flatpickr/dist/flatpickr.min.js",
              "node_modules/highlightjs/highlight.pack.min.js",
              "node_modules/jquery-mask-plugin/dist/jquery.mask.min.js",
              "node_modules/list.js/dist/list.min.js",
              "node_modules/quill/dist/quill.min.js",
              "node_modules/select2/dist/js/select2.full.min.js",
              "src/assets/libs/chart.js/Chart.extension.js",
              "src/assets/js/polyfills.js",
              "src/assets/js/charts.js",
              "src/assets/js/autosize.js",
              "src/assets/js/dashkit.js",
              "src/assets/js/dropdowns.js",
              "src/assets/js/dropzone.js",
              "src/assets/js/flatpickr.js",
              "src/assets/js/highlight.js",
              "src/assets/js/kanban.js",
              "src/assets/js/list.js",
              "src/assets/js/map.js",
              "src/assets/js/navbar.js",
              "src/assets/js/popover.js",
              "src/assets/js/quill.js",
              "src/assets/js/select2.js",
              "src/assets/js/tooltip.js",
              "src/assets/js/checklist.js",
              "src/assets/js/wizard.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "fc-rckb-frontend:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "fc-rckb-frontend:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "fc-rckb-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "fc-rckb-frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "fc-rckb-frontend:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "fc-rckb-frontend"
}
